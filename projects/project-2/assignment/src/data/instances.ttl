#Task 1
ex:A320_Neo_Spec rdf:type [
    a owl:Class ;
    owl:intersectionOf (
        [ a owl:Restriction ;
          owl:onProperty ex:is_about ;
          owl:allValuesFrom [
              a owl:Class ;
              owl:intersectionOf (
                  [ a owl:Restriction ;
                    owl:onProperty ex:has_continuant_part ;
                    owl:hasValue ex:Jet_Engine_1
                  ]
                  [ a owl:Restriction ;
                    owl:onProperty ex:has_continuant_part ;
                    owl:hasValue ex:Jet_Engine_2
                  ]
                  [ a owl:Restriction ;
                    owl:onProperty ex:has_manufacturer ;
                    owl:hasValue ex:Airbus
                  ]
                  [ a owl:Restriction ;
                  owl:onProperty ex:has_Model_BADA ;
                  owl:hasValue "Airbus A320-271N"^^xsd:string
                  ]
                  [ a owl:Restriction ;
                  owl:onProperty ex:has_Model_FAA ;
                  owl:hasValue "Airbus A320 Neo"^^xsd:string
                  ]
                  [ a owl:Restriction ;
                  owl:onProperty ex:has_Length_Ft ;
                  owl:hasValue "50.2"^^xsd:decimal
                  ]
                  [ a owl:Restriction ;
                  owl:onProperty ex:has_Tail_Height_Ft ;
                  owl:hasValue "29.4"^^xsd:decimal
                  ]
                  ex:Fixed_Wing

              )
          ]
        ]
    )
] .

ex:A320_Neo_Spec rdf:type ex:Artifact_Design ;
                 rdfs:label "Airbus A320 NEO"@en .

ex:A320_Neo_Spec a ex:Artifact_Design ;
    rdfs:label "Airbus A320 NEO"@en ;
    ex:has_manufacturer ex:Airbus ;
    ex:has_Model_BADA "Airbus A320-271N"^^xsd:string ;
    ex:has_Model_FAA "Airbus A320 Neo"^^xsd:string ;
    ex:has_Length_Ft "50.2"^^xsd:decimal ;
    ex:has_Tail_Height_Ft "29.4"^^xsd:decimal ;
    rdfs:subClassOf ex:Artifact_Design ;
    rdfs:label "Airbus A320 NEO"@en ;
    ex:has_manufacturer ex:Airbus ;
    ex:has_Model_BADA "Airbus A320-271N"^^xsd:string ;
    ex:has_Model_FAA "Airbus A320 Neo"^^xsd:string ;
    ex:has_Length_Ft "50.2"^^xsd:decimal ;
    ex:has_Tail_Height_Ft "29.4"^^xsd:decimal ;
    ex:has_continuant_part ex:Jet_Engine_1 ;
    ex:has_continuant_part ex:Jet_Engine_2 .

    ex:Jet_Engine_1 rdf:type ex:Jet_Engine .
    ex:Jet_Engine_2 rdf:type ex:Jet_Engine .

    ex:Airbus rdfs:label "Airbus"@en .
